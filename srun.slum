#!/bin/bash
#SBATCH -o srun.log               # 屏幕上的输出文件重定向到 srun.log
#SBATCH -p general                    # 作业提交的分区为 general
#SBATCH -c 32         # 单任务使用的 CPU 核心数为 4
#SBATCH -N 1                      # 作业申请 1 个节点
#SBATCH -n 1
#SBATCH --mem=20000           # MB Memory per node
#SBATCH -t 0-99:00:00                # 任务运行的最长时间为 1 小时

./geos.mp                 # 执行命令
